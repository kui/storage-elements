{
  "name": "storage-form",
  "version": "1.0.0",
  "description": "Custom element to use <form> as storages such as localStorage, chrome.storage.",
  "main": "lib/storage-form.js",
  "scripts": {
    "test": "npm run check && mocha --compilers js:babel-register test",
    "lib": "babel src -d lib",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "flow": "flow check src & flow check test & wait",
    "check": "npm run flow & npm run lint & wait",
    "debug-dist": "webpack",
    "prod-dist": "NODE_ENV=production webpack",
    "dist": "npm run debug-dist & npm run prod-dist & wait",
    "docs": "npm run check && npm run dist && mkdir -vp docs && cp -v dist/* docs",
    "serve": "webpack-dev-server --output-path docs --content-base docs --open",
    "clean": "rm -fr lib dist"
  },
  "keywords": [
    "web",
    "web-components",
    "custom-elements",
    "stroage",
    "web-stroage",
    "chrome-storage"
  ],
  "author": "Keiichiro Ui",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-strip-function-call": "^1.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-unassert": "^2.1.2",
    "babel-preset-babili": "0.0.9",
    "babel-preset-env": "^1.1.1",
    "babel-register": "^6.18.0",
    "babili-webpack-plugin": "0.0.7",
    "eslint": "^3.12.2",
    "eslint-config-babel": "^1.0.2",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "flow-bin": "^0.37.0",
    "flow-typed": "^2.0.0",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "engine": {
    "node": "^6.x"
  },
  "repository": "https://github.com/kui/storage-elements.git"
}
